<p id=”alert”><%= alert %></p>

<h1>Log In</h1>
<%= form_tag sessions_path do |form| %>

    <%= label_tag :email %><br />
    <%= text_field_tag :email %><br />

    <%= label_tag :password %><br />
    <%= password_field_tag :password %><br /><br />

    <%= submit_tag "Log In" %><br />

    <p><strong>Don't have an account yet?</strong></p>
    <%= link_to 'Sign up', signup_path %>

<% end %>
